<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.thinkgem.jeesite.modules.sys.dao.FxGoodsSeasonDao">
    
	<sql id="fxGoodsSeasonColumns">
		a.season_id AS "seasonId",
		a.season_name AS "seasonName"
	</sql>
	
	<sql id="fxGoodsSeasonJoins">
	</sql>
    
	<select id="get" resultType="FxGoodsSeason">
		SELECT 
			<include refid="fxGoodsSeasonColumns"/>
		FROM fx_goods_season a
		<include refid="fxGoodsSeasonJoins"/>
		WHERE a.id = #{id}
	</select>
	
	<select id="findList" resultType="FxGoodsSeason">
		SELECT 
			<include refid="fxGoodsSeasonColumns"/>
		FROM fx_goods_season a
		<include refid="fxGoodsSeasonJoins"/>
		<where>
			
		</where>
		<choose>
			<when test="page !=null and page.orderBy != null and page.orderBy != ''">
				ORDER BY ${page.orderBy}
			</when>
			<otherwise>
			</otherwise>
		</choose>
	</select>
	
	<select id="findAllList" resultType="FxGoodsSeason">
		SELECT 
			<include refid="fxGoodsSeasonColumns"/>
		FROM fx_goods_season a
		<include refid="fxGoodsSeasonJoins"/>
		<where>
			
		</where>		
		<choose>
			<when test="page !=null and page.orderBy != null and page.orderBy != ''">
				ORDER BY ${page.orderBy}
			</when>
			<otherwise>
			</otherwise>
		</choose>
	</select>
	
	<insert id="insert">
		INSERT INTO fx_goods_season(
			season_id,
			season_name
		) VALUES (
			#{seasonId},
			#{seasonName}
		)
	</insert>
	
	<update id="update">
		UPDATE fx_goods_season SET 	
			season_id = #{seasonId},
			season_name = #{seasonName}
		WHERE id = #{id}
	</update>
	
	<update id="delete">
		DELETE FROM fx_goods_season
		WHERE id = #{id}
	</update>
	
</mapper>